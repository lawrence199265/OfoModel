<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTabLayout">
        <attr name="indicatorColor" />
        <attr name="textSize" />
        <attr name="textGravity" />

    </declare-styleable>

    <declare-styleable name="MyTabItem">
        <attr name="icon" />
        <attr name="indicatorColor" />
        <attr name="text" />
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="textGravity" />
    </declare-styleable>

    <attr name="indicatorColor" format="color" />
    <attr name="text" format="string" />
    <attr name="textSize" format="dimension" />
    <attr name="textColor" format="color" />
    <attr name="textGravity" format="enum">
        <enum name="top" value="48" />
        <enum name="left" value="3" />
        <enum name="right" value="5" />
        <enum name="bottom" value="80" />
    </attr>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <declare-styleable name="SwipeCaptchaView">
        <attr name="captchaWidth" format="dimension" />
        <attr name="captchaHeight" format="dimension" />
        <attr name="matchDeviation" format="dimension" />
    </declare-styleable>
    <declare-styleable name="XEditText">
        <attr name="x_separator" format="string" />
        <attr name="x_disableClear" format="boolean" />
        <attr name="x_clearDrawable" format="reference" />
        <attr name="x_showPwdDrawable" format="reference" />
        <attr name="x_hidePwdDrawable" format="reference" />
        <attr name="x_disableEmoji" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="unitHeight" format="dimension" />
        <attr name="itemNumber" format="integer" />

        <attr name="normalTextColor" format="color" />
        <attr name="normalTextSize" format="dimension" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectedTextSize" format="dimension" />

        <attr name="lineColor" format="color" />
        <attr name="lineHeight" format="dimension" />

        <attr name="maskHeight" format="dimension" />
        <attr name="noEmpty" format="boolean" />
        <attr name="isEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LinearLayoutListItemViewStyle">
        <!--左侧图标 -->
        <attr name="icon_lefttext" format="reference" />

        <!--左侧图标 -->
        <attr name="icon_left" format="reference" />
        <!--右侧图标 -->
        <attr name="icon_right" format="reference" />
        <!--右侧图标2 -->
        <attr name="icon_right2" format="reference" />
        <!--左侧文本 -->
        <attr name="text_left" format="reference|string" />
        <!--左侧文本 -->
        <attr name="text_left2" format="reference|string" />
        <!--左侧角标文本 -->
        <attr name="text_left_superscript" format="reference|string" />
        <!--中间文本 -->
        <attr name="text_middle" format="reference|string" />
        <!--右侧文本 -->
        <attr name="text_right" format="reference|string" />
        <!--右侧文本2 -->
        <attr name="text_right2" format="reference|string" />
        <!--右侧文本3 -->
        <attr name="text_right3" format="reference|string" />
        <!--底部文本 -->
        <attr name="text_bottom" format="reference|string" />
        <!-- 右侧图标是否使用字体文件 -->
        <attr name="icon_text_right" format="reference|boolean" />

        <attr name="text_edittext_content" format="reference|string" />
        <attr name="text_edittext_hint" format="reference|string" />

        <!--左侧文本边距 -->
        <attr name="text_content_margin_left" format="reference|integer" />
        <!--右侧文本边距-->
        <attr name="text_content_margin_right" format="reference|integer" />

        <!--左侧文本字体大小 -->
        <attr name="text_left_fontsize" format="reference|integer" />
        <!--左侧文本2字体大小 -->
        <attr name="text_left2_fontsize" format="reference|integer" />
        <!--中间文本字体大小 -->
        <attr name="text_middle_fontsize" format="reference|integer" />
        <!--右侧文本字体大小-->
        <attr name="text_right_fontsize" format="reference|integer" />
        <!--右侧文本2字体大小-->
        <attr name="text_right2_fontsize" format="reference|integer" />
        <!--右侧文本3字体大小-->
        <attr name="text_right3_fontsize" format="reference|integer" />
        <!--底部文本字体大小-->
        <attr name="text_bottom_fontsize" format="reference|integer" />

        <!--文本字体大小 -->
        <attr name="text_edittext_fontsize" format="reference|integer" />

        <!--左侧文本字体颜色 -->
        <attr name="text_left_fontcolor" format="reference|color" />
        <!--左侧文本2字体颜色 -->
        <attr name="text_left2_fontcolor" format="reference|color" />
        <!--中间文本字体颜色 -->
        <attr name="text_middle_fontcolor" format="reference|color" />
        <!--右侧文本字体颜色-->
        <attr name="text_right_fontcolor" format="reference|color" />
        <!--右侧文本2字体颜色-->
        <attr name="text_right2_fontcolor" format="reference|color" />
        <!--右侧文本3字体颜色-->
        <attr name="text_right3_fontcolor" format="reference|color" />
        <!--底部文本字体颜色-->
        <attr name="text_bottom_fontcolor" format="reference|color" />

        <!--文本字体颜色 -->
        <attr name="text_edittext_fontcolor" format="reference|color" />

        <!--右侧文本背景-->
        <attr name="text_right_background" format="reference" />

        <!--左侧文本左边距 -->
        <attr name="text_margin_left" format="reference|integer" />
        <!--左侧文本右边距 -->
        <attr name="text_margin_left_right" format="reference|integer" />
        <!--左侧文本2左边距 -->
        <attr name="text_margin_left2_left" format="reference|integer" />
        <!--左侧文本2右边距 -->
        <attr name="text_margin_left2_right" format="reference|integer" />
        <!--右侧文本左边距-->
        <attr name="text_margin_right_left" format="reference|integer" />
        <!--右侧文本右边距-->
        <attr name="text_margin_right" format="reference|integer" />

        <!-- 左2文本是否单行显示 -->
        <attr name="text_left2_single_line" format="reference|boolean" />
        <!-- 左侧文本角标是否显示 -->
        <attr name="text_left_superscript_show" format="reference|boolean" />
        <!-- 右侧文本是否单行显示 -->
        <attr name="text_right_single_line" format="reference|boolean" />

        <!-- 设置是否可以单击 -->
        <attr name="clickable" format="reference|boolean" />
        <!--消息数字 -->
        <attr name="text_messagge_number" format="reference|string" />

        <attr name="list_item_style">
            <!--只显示文本,不允许点击 -->
            <enum name="listitem_text" value="0" />
            <!--允许点击 -->
            <enum name="listitem_menu" value="1" />
        </attr>

        <attr name="list_item_line_show_model">
            <!--显示:上边线-->
            <enum name="listitem_show_no_line" value="0" />
            <!--显示:上边线-->
            <enum name="listitem_show_top_line" value="1" />
            <!--显示:下边线-->
            <enum name="listitem_show_bottom_line" value="2" />
            <!--显示:上下边线-->
            <enum name="listitem_show_top_bottom_line" value="3" />
        </attr>

        <!--上边线高度 单位:px-->
        <attr name="list_item_top_line_height_size" format="reference|integer" />
        <!--下边线高度 单位:px-->
        <attr name="list_item_bottom_line_height_size" format="reference|integer" />

        <!--上边线宽度 单位:%-->
        <attr name="list_item_top_line_width_size" format="reference|integer" />
        <!--下边线宽度 单位:%-->
        <attr name="list_item_bottom_line_width_size" format="reference|integer" />
        <!-- Item高度 单位:dp-->
        <attr name="list_item_height" format="reference|integer" />
        <!-- 左侧头像尺寸 单位:dp -->
        <attr name="list_item_image_size" format="reference|integer" />
    </declare-styleable>
    <declare-styleable name="RippleView">
        <attr name="rv_color" format="color" />
        <attr name="rv_alpha" format="integer" />
        <attr name="rv_frame_rate" format="integer" />
        <attr name="rv_ripple_duration" format="integer" />
        <attr name="rv_zoom_duration" format="integer" />
        <attr name="rv_centered" format="boolean" />
        <attr name="rv_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="rv_ripple_padding" format="dimension" />
        <attr name="rv_zoom" format="boolean" />
        <attr name="rv_zoom_scale" format="float" />
        <attr name="rv_enable_ripple" format="boolean" />

    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="image_mode">
            <!--显示:正常图片-->
            <enum name="image_normal" value="0" />
            <!--显示:圆形图-->
            <enum name="image_round" value="1" />
            <!--显示:带边框圆形图，圆边的宽度由border_thickness决定-->
            <enum name="image_round_with_circle" value="2" />
            <!--显示:倒角对圆形 -->
            <enum name="image_round_chamfer" value="3" />
            <!--显示:切圆，画布加边，右下角45度加阴影-->
            <enum name="image_round_shadow" value="4" />
        </attr>

    </declare-styleable>


    <declare-styleable name="ParticipantView">

        <attr name="headSpacing" format="dimension" />
        <attr name="omitSpacing" format="dimension" />
        <attr name="omitCenter" format="boolean" />
    </declare-styleable>

</resources>